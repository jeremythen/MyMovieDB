{
  "name": "mymoviedb",
  "version": "1.0.0",
  "description": "Digital platform to store movies and manage reviews and ratings",
  "main": "app.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test npx sequelize-cli db:create && cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo:all && cross-env NODE_ENV=test npx sequelize-cli db:migrate && cross-env NODE_ENV=test npx sequelize-cli db:seed:all && cross-env NODE_ENV=test mocha -r ts-node/register src/test/*",
    "tsc": "npx tsc",
    "start": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "prod": "npx tsc && node ./build/app.js",
    "docker-build": "env NODE_ENV=docker && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all && npm run prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremythen/MyMovieDB.git"
  },
  "author": "Jeremy Then",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeremythen/MyMovieDB/issues"
  },
  "homepage": "https://github.com/jeremythen/MyMovieDB#readme",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.6.0",
    "@types/redis": "^2.8.26",
    "@types/sequelize": "^4.28.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-status-codes": "^1.4.0",
    "inversify": "^5.0.1",
